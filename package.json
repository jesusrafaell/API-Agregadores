{
  "name": "api-tranred",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "config": {
    "env": ".env.development"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:qa": "env-cmd -f .env.development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:qa": "env-cmd -f .env.development node dist/main",
    "start:prod": "env-cmd -f .env.production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sitran:create": "npx typeorm-ts-node-esm migration:generate ./src/db/migrations/Migratrion -d ./src/db/config/sitran_dataSource.ts",
    "gscomputer:create": "npx typeorm-ts-node-esm migration:generate ./src/db/migrations/Migratrion -d ./src/db/config/dataSource/gscomputer.ts",
    "migrate:create": "yarn gscomputer:create",
    "sitran:run": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:run -d ./src/db/config/sitran_dataSource.ts",
    "librepago:run": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:run -d ./src/db/config/dataSource/librePago.ts",
    "carropago:run": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:run -d ./src/db/config/dataSource/carroPago.ts",
    "milPago:run": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:run -d ./src/db/config/dataSource/milpago.ts",
    "gscomputer:run": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:run -d ./src/db/config/dataSource/gscomputer.ts",
    "disglobal:run": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:run -d ./src/db/config/dataSource/disglobal.ts",
    "consultel:run": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:run -d ./src/db/config/dataSource/consultel.ts",
    "sitran:revert": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:revert -d ./src/db/config/sitran_dataSource.ts",
    "milpagos:revert": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:revert -d ./src/db/config/dataSource/milPago.ts",
    "carropago:revert": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:revert -d ./src/db/config/dataSource/carroPago.ts",
    "librepago:revert": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:revert -d ./src/db/config/dataSource/librePago.ts",
    "gscomputer:revert": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:revert -d ./src/db/config/dataSource/gscomputer.ts",
    "disglobal:revert": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:revert -d ./src/db/config/dataSource/disglobal.ts",
    "consultel:revert": "env-cmd -f %npm_package_config_env% npx typeorm-ts-node-esm migration:revert -d ./src/db/config/dataSource/consultel.ts",
    "migrate:preData": "env-cmd -f %npm_package_config_env% ts-node src/db/contents/index.ts",
    "migrate:show": "npx typeorm-ts-node-esm migration:show -d ./src/db/config/migracion.ts"
  },
  "dependencies": {
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "axios": "^1.2.0",
    "bcrypt": "^5.1.0",
    "child_process": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dataloader": "^2.1.0",
    "env-cmd": "^10.1.0",
    "luxon": "^3.0.3",
    "nodemon": "^2.0.19",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.0.11",
    "typeorm": "^0.3.9",
    "voyager": "^0.4.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.3",
    "@types/luxon": "^3.0.1",
    "@types/node": "^18.11.10",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "mssql": "^9.0.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
